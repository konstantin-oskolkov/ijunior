static void Main(string[] args)
      {
         int checkValue = 100;
         int limit = 1000;
         int count = 0;
         
         Console.Write("Введите число n: ");
         int number = Convert.ToInt32(Console.ReadLine());         

         for (int i = number; i < limit; i = i + number)
         {
            if (i >= checkValue)
            {
               count++;
            }
         }
         Console.WriteLine(count);
      }
