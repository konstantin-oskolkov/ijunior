static void Main(string[] args)
      {
         Console.Write("Введите число n: ");
         int n = Convert.ToInt32(Console.ReadLine());

         int count = 0;

         for (int i = n; i < 1000; i = i + n)
         {
            if (i >= 100)
            {
               count++;
            }
         }
         Console.WriteLine(count);
      }
