   class Program
   {
      static void Main(string[] args)
      {
         Shuffle();
      }

      static void Shuffle()
      {
         int[] massiveOfNumbers = { 0, 22, 11, 33, 4, 5, 66, 7, 88, 9 };
         Random rnd = new Random();

         for (int i = massiveOfNumbers.Length - 1; i >= 0; i--)
         {
            int randomIndex = rnd.Next(i + 1);
            int shuffleIndex = massiveOfNumbers[randomIndex];

            massiveOfNumbers[randomIndex] = massiveOfNumbers[i];
            massiveOfNumbers[i] = shuffleIndex;

            Console.WriteLine(massiveOfNumbers[i]);
         }
      }
   }
